  {
    "name": "Multimodal Web Scraper",
    "version": "1.0.0",
    "description": "Multimodal web scraping via gaze-detection and speech recognition",
    "manifest_version": 2,
    "content_scripts": [
      {
        "js": [
          "./modules/webgazer/tensorflow.js",
          "./modules/webgazer/webgazer.js",
          "./modules/gaze-recognition.js",
          "./index.js"
        ],
        "matches": [
          "http://localhost:8080/",
          "https://helena-lang.org/sample-data",
          "https://scholar.google.com/citations?user=*",
          "https://www.csail.mit.edu/people?*"
        ]
      }
    ],
    "background": {
      "scripts": [
        "./background.js"
      ]
    },
    "permissions": [
      "*://localhost/*"
    ]
  }